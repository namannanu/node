  "testScript": {
    "javascript": [
      "async function testPresignedUrls() {",
      "  const testCases = [",
      "    {",
      "      name: 'Test with MongoDB _id',",
      "      userId: '68a9902e4f0701bb871e1f3e'",
      "    },",
      "    {",
      "      name: 'Test with custom userId',",
      "      userId: 'user-rgdr58npg-meo33m5p'",
      "    },",
      "    {",
      "      name: 'Test with non-existent user',",
      "      userId: 'nonexistent-user'",
      "    }",
      "  ];",
      "",
      "  for (const test of testCases) {",
      "    console.log(`Running ${test.name}`);",
      "    try {",
      "      const response = await fetch(`https://correct-eight.vercel.app/api/users/${test.userId}/presigned-urls`, {",
      "        headers: {",
      "          'Authorization': `Bearer ${localStorage.getItem('token')}`",
      "        }",
      "      });",
      "      const data = await response.json();",
      "      console.log(`${test.name} Result:`, data);",
      "    } catch (error) {",
      "      console.error(`${test.name} Error:`, error);",
      "    }",
      "  }",
      "}"
    ]
  },
